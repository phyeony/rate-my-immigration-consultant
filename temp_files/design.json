{
    "theme": {
      "colors": {
        "primary": "#337ab7",
        "secondary": "#f9f9f9",
        "text": "#333333",
        "textSecondary": "#555555",
        "background": "#ffffff",
        "border": "#dddddd",
        "verifiedBlue": "#007bff"
      },
      "fonts": {
        "family": {
          "body": "Helvetica, Arial, sans-serif",
          "heading": "Helvetica, Arial, sans-serif"
        },
        "size": {
          "base": "16px",
          "small": "14px",
          "large": "18px",
          "h1": "24px",
          "h2": "20px"
        }
      },
      "spacing": {
        "small": "8px",
        "medium": "16px",
        "large": "24px"
      },
      "borderRadius": "8px"
    },
    "components": {
      "header": {
        "style": "flex justify-between items-center p-4 border-b",
        "logo": {
          "component": "Logo",
          "text": "Psychology Today",
          "style": "text-2xl font-bold text-primary"
        },
        "searchBar": {
          "component": "Input",
          "placeholder": "City, Postcode or Name",
          "style": "border rounded-md py-2 px-4 w-1/3"
        },
        "navLinks": {
          "component": "Nav",
          "links": ["Log In", "Sign Up and Get Listed"],
          "style": "flex gap-4 items-center"
        }
      },
      "button": {
        "baseStyle": "py-2 px-4 rounded-md font-semibold text-center",
        "variants": {
          "primary": {
            "style": "bg-primary text-white hover:bg-blue-700"
          },
          "secondary": {
            "style": "bg-gray-200 text-gray-800 hover:bg-gray-300"
          },
          "outline": {
            "style": "border border-primary text-primary hover:bg-blue-50"
          }
        }
      },
      "therapistCard": {
        "containerStyle": "flex p-4 border rounded-lg shadow-md mb-4 gap-4 items-start",
        "image": {
          "style": "w-24 h-24 rounded-full object-cover"
        },
        "detailsContainer": {
          "style": "flex-1"
        },
        "name": {
          "style": "text-xl font-bold text-primary flex items-center gap-2",
          "verifiedIcon": {
            "component": "Icon",
            "name": "check-circle",
            "style": "text-verifiedBlue"
          }
        },
        "title": {
          "style": "text-textSecondary"
        },
        "availability": {
          "style": "text-textSecondary text-sm"
        },
        "description": {
          "style": "mt-2 text-text"
        },
        "actionsContainer": {
          "style": "flex flex-col gap-2 w-48"
        }
      },
      "filterChip": {
        "style": "bg-gray-100 border border-gray-300 text-gray-700 rounded-full py-1 px-4 text-sm cursor-pointer hover:bg-gray-200"
      },
      "profilePage": {
        "header": {
          "style": "flex items-center gap-4 mb-6"
        },
        "name": {
          "style": "text-2xl font-bold"
        },
        "title": {
          "style": "text-lg text-textSecondary"
        },
        "verified": {
          "style": "flex items-center gap-1 text-sm text-textSecondary",
          "icon": "check"
        },
        "contactInfo": {
          "style": "border rounded-lg p-4 bg-white"
        },
        "sessionInfo": {
          "style": "flex justify-between items-center mt-4 text-lg"
        }
      },
      "form": {
        "containerStyle": "max-w-md mx-auto p-8 border rounded-lg shadow-lg bg-white",
        "input": {
          "labelStyle": "block text-sm font-medium text-gray-700 mb-1",
          "inputStyle": "block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-primary focus:border-primary"
        },
        "dropdown": {
          "style": "block w-full px-3 py-2 border border-gray-300 bg-white rounded-md shadow-sm focus:outline-none focus:ring-primary focus:border-primary"
        },
        "checkbox": {
          "containerStyle": "flex items-center",
          "labelStyle": "ml-2 block text-sm text-gray-900"
        }
      }
    },
    "pages": {
      "searchResult": {
        "layout": "container mx-auto px-4 py-8",
        "header": {
          "title": "Therapists in [Location]",
          "style": "text-2xl font-bold mb-4"
        },
        "filters": {
          "component": "FilterSidebar"
        },
        "results": {
          "component": "TherapistList"
        }
      },
      "therapistProfile": {
        "layout": "container mx-auto px-4 py-8 max-w-4xl"
      },
      "signup": {
        "layout": "flex justify-center items-center min-h-screen bg-gray-50",
        "formContainer": {
          "component": "form"
        }
      }
    }
  }